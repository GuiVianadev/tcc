{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx --env-file .env --experimental-strip-types --no-warnings --watch src/server.ts",
    "build": "tsup",
    "build:watch": "tsc --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed:admin": "tsx --env-file .env --experimental-strip-types src/db/seed.ts",
    "db:seed:prod": "node build/seed.cjs",
    "start": "node build/server.cjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@ai-sdk/google": "^2.0.20",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/multipart": "^9.2.1",
    "1.1.1": "^1.0.0",
    "ai": "^5.0.68",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.18",
    "decompress": "^4.2.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "drizzle-kit": "^0.31.5",
    "fastify": "^5.6.1",
    "mammoth": "^1.11.0",
    "pdf-parse": "^2.2.9",
    "pg": "^8.16.3",
